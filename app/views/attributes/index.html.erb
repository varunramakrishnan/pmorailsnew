<p id="notice"><%= notice %></p>

<h1>Listing Attributes</h1>

<table>
  <thead>
    <tr>
      <th>Attribute type</th>
      <th>Attribute key</th>
      <th>Attribute value</th>
      <th>Active status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attributes.each do |attribute| %>
      <tr>
        <td><%= attribute.attribute_type %></td>
        <td><%= attribute.attribute_key %></td>
        <td><%= attribute.attribute_value %></td>
        <td><%= attribute.active_status %></td>
        <td><%= link_to 'Show', attribute %></td>
        <td><%= link_to 'Edit', edit_attribute_path(attribute) %></td>
        <td><%= link_to 'Destroy', attribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attribute', new_attribute_path %>
